<p id="notice"><%= notice %></p>
<h1>Import Quotes</h1>

<%= form_tag import_quotes_path, multipart: true do %>
  <div>
    <%= select_tag "category_id", options_from_collection_for_select(@categories, "id", "name") %>
  </div>
  </br>
  <div>
    <%= file_field_tag :file %>
  </div>
  </br>
  <div>
    <%= submit_tag "Import" %>
  </div>
<% end %>